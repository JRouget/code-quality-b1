{
  "name": "com/clean-tests",
  "type": "library",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Com\\CleanTests\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit tests",
    "test-coverage": "phpunit --coverage-html build/coverage"
  }
}
